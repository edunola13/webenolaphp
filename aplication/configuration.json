{
    "base_url": "http://localhost/webenolaphp/",
    "index_page": "",
    "environment": "development",
    "calculate_performance": "FALSE",
    "configuration": "configuration/",
    "charset": "UTF-8",
      
    "controllers": {
        "index": {"class": "Index", "url": ""},
        "download": {"class": "Index", "url": "download"},
        "contribute": {"class": "Index", "url": "contribute"},
        "contact": {"class": "Index", "url": "contact"},
        "documentation": {"class": "Documentation", "url": "manual/*"},
        "adminDoc": {"class": "admin/AdminDoc", "url": "adminDoc/-"},
        "login": {"class": "Login", "url": "login"},
        "logout": {"class": "Logout", "url": "logout"}
    },

    "filters": {
        "authorization":{"class":"Authorization", "filtered": "/*"}
    },
            
    "filters_after_processing": {
    },
            
    "i18n": {
        "default": "en",
        "locales": "es"
    },
       
    "url-components": "enola-components",
    "components":{
    },
            
    "database": {
        "configuration": "database.json"
    },
                
    "libraries": {
        "uiservices":{"class":"ServiciosUI-1.0-Eval/servicioui"}
    }
}